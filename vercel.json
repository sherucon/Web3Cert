{
    "version": 2,
    "builds": [
        {
            "src": "api/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ],
    "env": {
        "PRIVATE_KEY": "PRIVATE_KEY",
        "AMOY_RPC_URL": "AMOY_RPC_URL",
        "PINATA_API_KEY": "PINATA_API_KEY",
        "PINATA_SECRET_KEY": "PINATA_SECRET_KEY",
        "CONTRACT_ADDRESS": "CONTRACT_ADDRESS"
    }
}